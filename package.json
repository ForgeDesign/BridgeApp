    "babel-core": "^7.0.0-bridge.0",
    "color": "2.0.1",
    "hoist-non-react-statics": "^2.5.0",
    "metro-react-native-babel-preset": "^0.50.0",
    "moment": "^2.19.4",
    "native-base": "^2.7.0",
    "npm": "^5.10.0",
    "plist": "^3.0.1",
    "prop-types": "^15.6.1",
    "react": "16.6.3",
    "react-native": "0.57.7",
    "react-native-atoz-listview": "github:agates4/react-native-atoz-listview",
    "react-native-awesome-alerts": "^1.0.7",
    "react-native-citycheck-flip-view": "github:agates4/react-native-flip-view",
    "react-native-color": "0.0.10",
    "react-native-deep-link": "^0.2.3",
    "react-native-deep-linking": "^2.1.0",
    "react-native-dropdownalert": "^3.4.0",
    "react-native-easy-grid": "^0.2.0",
    "react-native-elements": "^0.19.0",
    "react-native-extended-stylesheet": "^0.8.1",
    "react-native-facebook-login": "^1.6.1",
    "react-native-fbsdk": "^0.8.0",
    "react-native-firebase": "^5.1.1",
    "react-native-flip-card": "^3.5.2",
    "react-native-geocoder": "^0.5.0",
    "react-native-google-sign-in": "^1.0.16",
    "react-native-hero": "^0.3.1",
    "react-native-iap": "^2.3.21",
    "react-native-image-crop-picker": "^0.20.0",
    "react-native-image-picker": "github:curoo/react-native-image-picker",
