    "react-native-open-maps": "^0.1.4",
    "react-native-picker-select": "^2.0.1",
    "react-native-popup-dialog": "0.10.46",
    "react-native-prompt": "^1.0.0",
    "react-native-qrcode": "^0.2.6",
