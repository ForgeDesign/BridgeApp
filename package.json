{
  "name": "BridgeCard",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/preset-flow": "^7.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "react-test-renderer": "16.0.0"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "iosprod": "react-native run-ios --configuration Release",
    "iosx": "react-native run-ios --simulator \"iPhone X\"",
    "iosxprod": "react-native run-ios --simulator \"iPhone X\" --configuration Release",
    "device": "react-native run-ios --device",
    "deviceprod": "react-native run-ios --device --configuration Release",
    "cleanstart": "react-native start --reset-cache",
    "startclean": "react-native start --reset-cache",
    "lint": "eslint app/",
    "buildios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/BridgeCard/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "lint:fix": "eslint app/ --fix",
    "ios:setup": "node ./node_modules/react-native-fbsdk/bin/ios_setup.js",
    "postinstall": "cd ./node_modules/react-native && scripts/ios-install-third-party.sh && cd third-party && cd $(ls | grep 'glog' | awk '{print $1}') && ./configure && cd ../../../.. && node ./android-release-gradle-fix.js"
  },
  "dependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.2.0",
    "@babel/preset-env": "^7.2.0",
